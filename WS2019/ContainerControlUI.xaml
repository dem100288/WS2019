<UserControl x:Class="WS2019.ContainerControlUI"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:WS2019"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <StackPanel>
        <StackPanel Orientation="Horizontal">
            <TextBlock Name="textnum" Text="num"/>
            <TextBlock Name="textId"/>
        </StackPanel>
        <StackPanel Orientation="Horizontal">
            <Image Name="imageCap"/>
            <TextBlock Name="textCap"/>
        </StackPanel>
        <StackPanel Orientation="Horizontal">
            <TextBlock Name="textfine" Text="Штраф"/>
            <TextBlock Name="textFine"/>
        </StackPanel>
    </StackPanel>
</UserControl>
