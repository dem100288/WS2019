<UserControl x:Class="WS2019.DumpUI"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:WS2019"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300" Width="50" Height="50" RenderTransformOrigin="0.5,0.5" MouseUp="UserControl_MouseUp">
    <DockPanel>
        <TextBlock Name="textD" DockPanel.Dock="Top"/>
        <Image Name="imageD" Stretch="Fill"/>
    </DockPanel>
</UserControl>
